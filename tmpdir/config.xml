<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-200.0</x_min>
		<x_max>200.0</x_max>
		<y_min>-200.0</y_min>
		<y_max>200.0</y_max>
		<z_min>-10.0</z_min>
		<z_max>10.0</z_max>
		<dx>20.0</dx>
		<dy>20.0</dy>
		<dz>20.0</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200.0</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder>  

		<full_data>
			<interval units="min">60</interval>
			<enable>True</enable>
		</full_data>
		
		<SVG>
			<interval units="min">30</interval>
			<enable>True</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<microenvironment_setup>
		<variable ID="0" name="virion" units="virion/um^3">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">90.0</diffusion_coefficient>
				<decay_rate units="1/min">0.0</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="virion/um^3">0.0</initial_condition>
			<Dirichlet_boundary_condition enabled="false" units="virion/um^3">0.0</Dirichlet_boundary_condition>
		</variable>

		
		<variable ID="1" name="assembled virion" units="virion/um^3">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">90.0</diffusion_coefficient>
				<decay_rate units="1/min">0.0</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="virion/um^3">0.0</initial_condition>
			<Dirichlet_boundary_condition enabled="false" units="virion/um^3">0.0</Dirichlet_boundary_condition>
		</variable>

		<options>
			<calculate_gradients>True</calculate_gradients>
			<track_internalized_substrates_in_each_agent>True</track_internalized_substrates_in_each_agent>
			 
			<initial_condition enabled="false" type="matlab">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			 
			<dirichlet_nodes enabled="false" type="matlab">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>	
	
	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed> 
		 
		
<div_replication description="---Virus Replication---" type="divider" />		
		
		<virion_uncoating_rate description="rate at which an internalized virion is uncoated" type="double" units="1/min">0.01</virion_uncoating_rate>
		<uncoated_to_RNA_rate description="rate at which uncoated virion makes its mRNA available" type="double" units="1/min">0.01</uncoated_to_RNA_rate> 
		<protein_synthesis_rate description="rate at mRNA creates complete set of proteins" type="double" units="1/min">0.01</protein_synthesis_rate>
		<virion_assembly_rate description="rate at which viral proteins are assembled into complete virion" type="double" units="1/min">0.01</virion_assembly_rate>
		
<div_import_export description="---Virus Adsorption and Export---" type="divider" />		
		
		<virion_export_rate description="rate at which a virion is exported from a live cell" type="double" units="1/min">0.01</virion_export_rate> 

<div_receptors description="---ACE2 receptor dynamics with virus binding---" hidden="true" type="divider" />	

	<ACE2_receptors_per_cell description="number of ACE2 receptors per cell" hidden="true" type="double" units="dimensionless">1000</ACE2_receptors_per_cell>
	
	<ACE2_binding_rate description="ACE2 receptor-virus binding rate" hidden="true" type="double" units="1/min">0.001</ACE2_binding_rate> 

	<ACE2_endocytosis_rate description="ACE2 receptor-virus endocytosis rate" hidden="true" type="double" units="1/min">0.01</ACE2_endocytosis_rate> 
	
	<ACE2_cargo_release_rate description="ACE2 receptor-virus cargo release rate" hidden="true" type="double" units="1/min">0.001</ACE2_cargo_release_rate> 

	<ACE2_recycling_rate description="ACE2 receptor recycling rate" hidden="true" type="double" units="1/min">0.01</ACE2_recycling_rate> 
	

<div_apoptosis description="---Apoptotic Response---" hidden="true" type="divider" />		

	<max_infected_apoptosis_rate description="maximum rate of cell apoptosis due to viral infection" hidden="true" type="double" units="1/min">0.001</max_infected_apoptosis_rate>
	<max_apoptosis_half_max description="viral load at which cells reach half max apoptosis rate" hidden="true" type="double" units="virion">500</max_apoptosis_half_max>
	<apoptosis_hill_power description="Hill power for viral load apoptosis response" hidden="true" type="double" units="none">1</apoptosis_hill_power> 
	<virus_fraction_released_at_death description="fraction of internal virus released at cell death" hidden="true" type="double" units="none">0</virus_fraction_released_at_death>
		
<div_initialization description="---Initialization Options--" hidden="true" type="divider" />
	<multiplicity_of_infection description="multiplicity of infection: virions/cells at t=0" hidden="true" type="double" units="none">0.01</multiplicity_of_infection>
	<use_single_infected_cell description="Infect center cell with one virion (overrides MOI)" hidden="true" type="bool" units="none">false</use_single_infected_cell>

<div_visualization description="---Visualization Options---" type="divider" />
	<color_variable description="color cells based on this variable" type="string">assembled virion</color_variable>
		
	</user_parameters>
	
	
	
</PhysiCell_settings>